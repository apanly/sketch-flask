deb {{ common_apt_mirror }} {{ common_release_code }}-updates main multiverse restricted universe
deb {{ common_apt_mirror }} {{ common_release_code }}-proposed main multiverse restricted universe
deb {{ common_apt_mirror }} {{ common_release_code }}-backports main multiverse restricted universe
